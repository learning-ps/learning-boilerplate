{
    "packages": [
      "nixpkgs#curl",
      "./nix#boilerplate",
      "terraform@1.11.3",
      "terragrunt@0.76.6",
      "tflint@0.55.1",
      "go@1.23.1",
      "gotools@0.30.0"
    ],
    "env": {
      "GOPATH": "$HOME/go/",
      "PATH": "$PATH:$HOME/go/bin"
    },
    "shell": {
      "init_hook": [
        "export \"GOROOT=$(go env GOROOT)\""
      ]
    }
  }
  